{
  "manifest_version": 2,

  "name": "focus",
  "description": "focus",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://www.vmall.com/member/enterprise*"],
      "js": ["autoload.js"]
    }
  ],
  "permissions": [
    "https://www.vmall.com/member/enterprise*",
	"storage"
  ]
}